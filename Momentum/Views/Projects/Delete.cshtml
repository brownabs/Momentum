@model Momentum.Models.Project

@{
    ViewData["Title"] = "Delete";
}



<h3>Are you sure you want to delete this project?</h3>
    <hr />

    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateCreated)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateCreated)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DurationGoal)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DurationGoal)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ProjectName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ProjectName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Language)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Language)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.SourceCodeLink)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SourceCodeLink)
        </dd>
        @if (@Model.PublishedApplicationLink != null)
        {

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PublishedApplicationLink)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PublishedApplicationLink)
            </dd>
        }
        </dd>
        @if (@Model.ImagePath != null)
        {

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ImagePath)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ImagePath)
            </dd>
        }
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="ProjectId" />
        <input type="submit" value="Delete" class="btn btn-secondary" /> |
        <a asp-action="Index">Back to List  class="links"</a>
    </form>
</div>
